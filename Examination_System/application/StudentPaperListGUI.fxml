<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXDrawer?>
<?import com.jfoenix.controls.JFXHamburger?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="580.0" prefWidth="960.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.StudentPaperListController">
   <children>
      <JFXDrawer fx:id="Drawer" defaultDrawerSize="200.0" layoutY="35.0" prefHeight="545.0" prefWidth="200.0" />
      <HBox prefHeight="35.0" prefWidth="960.0" style="-fx-background-color: #494949;">
         <children>
            <JFXHamburger fx:id="Hamburger" prefHeight="40.0" prefWidth="50.0" />
            <Label prefHeight="35.0" prefWidth="350.0" text="  View Papers List" textFill="#e5e5e5">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Label>
            <Label fx:id="UIDlbl" prefHeight="35.0" prefWidth="200.0" text="User ID" textFill="#e5e5e5">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Label>
            <Label fx:id="systemTimelbl" prefHeight="35.0" prefWidth="300.0" text="Date &amp; Time" textFill="#e5e5e5">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Label>
            <JFXButton contentDisplay="CENTER" onMouseClicked="#handleMinimize" prefHeight="35.0" prefWidth="35.0" textFill="#0000000b">
               <graphic>
                  <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../image/icons8_Minimize_Window_100px.png" />
                     </image>
                  </ImageView>
               </graphic>
            </JFXButton>
            <JFXButton contentDisplay="CENTER" onMouseClicked="#handleClose" prefHeight="35.0" prefWidth="35.0" textFill="#0000000b">
               <graphic>
                  <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../image/icons8_Multiply_100px_1.png" />
                     </image>
                  </ImageView>
               </graphic>
            </JFXButton>
         </children>
      </HBox>
      <ImageView fitHeight="545.0" fitWidth="960.0" layoutY="38.0" opacity="0.3" pickOnBounds="true">
         <image>
            <Image url="@../image/jess-watters-483666-unsplash.jpg" />
         </image>
      </ImageView>
      <TableView fx:id="viewAnsPaperListTbl" layoutX="21.0" layoutY="58.0" prefHeight="506.0" prefWidth="918.0">
         <columns>
            <TableColumn fx:id="PaperIDcol" prefWidth="100.0" text="Paper ID" />
            <TableColumn fx:id="PaperPasswordcol" prefWidth="100.0" text="Password" />
            <TableColumn fx:id="ClassIDcol" prefWidth="100.0" text="Class ID" />
            <TableColumn fx:id="NoQecol" prefWidth="90.0" text="Questions" />
            <TableColumn fx:id="MarksCol" prefWidth="75.0" text="Marks" />
            <TableColumn fx:id="ansdatecol" prefWidth="150.0" text="Answered Time" />
            <TableColumn fx:id="releaseDateCol" prefWidth="150.0" text="Release Time" />
            <TableColumn fx:id="terminateDateCol" prefWidth="150.0" text="Terminate Time" />
         </columns>
      </TableView>
   </children>
</AnchorPane>
