<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.StudentHomeController">
   <children>
      <ImageView fitHeight="465.0" fitWidth="900.0" layoutY="34.0" opacity="0.4" pickOnBounds="true">
         <image>
            <Image url="@../image/jess-watters-483666-unsplash.jpg" />
         </image>
      </ImageView>
      <HBox layoutY="-1.0" prefHeight="35.0" prefWidth="900.0" style="-fx-background-color: #494949;">
         <children>
            <Label prefHeight="35.0" prefWidth="300.0" text="  Students Menue" textFill="#e5e5e5">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Label>
            <Label prefHeight="35.0" prefWidth="168.0" text="User ID" textFill="#e5e5e5">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Label>
            <Label prefHeight="35.0" prefWidth="369.0" text="Date and Time" textFill="#e5e5e5">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Label>
            <JFXButton fx:id="Minimizebttn3" contentDisplay="CENTER" onMouseClicked="#handleMinimize" prefHeight="35.0" prefWidth="35.0" textFill="#0000000b">
               <graphic>
                  <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../image/icons8_Minimize_Window_100px.png" />
                     </image>
                  </ImageView>
               </graphic>
            </JFXButton>
            <JFXButton fx:id="Closebttn3" contentDisplay="CENTER" onMouseClicked="#handleClose" prefHeight="35.0" prefWidth="35.0" textFill="#0000000b">
               <graphic>
                  <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../image/icons8_Multiply_100px_1.png" />
                     </image>
                  </ImageView>
               </graphic>
            </JFXButton>
         </children>
      </HBox>
      <JFXButton fx:id="AnswerPaperbtn" buttonType="RAISED" graphicTextGap="15.0" layoutX="44.0" layoutY="110.0" onMouseClicked="#MovetoSelectPaper" prefHeight="70.0" prefWidth="380.0" style="-fx-background-color: #7C7A7A;" text="Answer Paper" textFill="#e5e5e5">
         <font>
            <Font name="System Bold" size="25.0" />
         </font>
         <graphic>
            <ImageView fitHeight="37.0" fitWidth="41.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../image/icons8_Paper_96px.png" />
               </image>
            </ImageView>
         </graphic>
      </JFXButton>
      <JFXButton fx:id="ViewPaperbtn" buttonType="RAISED" graphicTextGap="20.0" layoutX="44.0" layoutY="240.0" onMouseClicked="#MovetoViewPaper" prefHeight="70.0" prefWidth="380.0" style="-fx-background-color: #7C7A7A;" text="View Paper" textFill="#e5e5e5">
         <font>
            <Font name="System Bold" size="25.0" />
         </font>
         <graphic>
            <ImageView fitHeight="34.0" fitWidth="31.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../image/icons8_View_100px.png" />
               </image>
            </ImageView>
         </graphic>
      </JFXButton>
      <JFXButton fx:id="ViewResultsbtn" buttonType="RAISED" graphicTextGap="20.0" layoutX="44.0" layoutY="370.0" onMouseClicked="#MovetoViewResults" prefHeight="70.0" prefWidth="380.0" style="-fx-background-color: #7C7A7A;" text="View Results" textFill="#e5e5e5">
