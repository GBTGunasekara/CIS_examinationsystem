            <Font name="System Bold" size="25.0" />
         </font>
         <graphic>
            <ImageView fitHeight="42.0" fitWidth="42.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../image/icons8_Classroom_100px.png" />
               </image>
            </ImageView>
         </graphic>
      </JFXButton>
      <JFXButton fx:id="StudentProbtn" alignment="CENTER" buttonType="RAISED" graphicTextGap="10.0" layoutX="480.0" layoutY="370.0" nodeOrientation="LEFT_TO_RIGHT" onMouseClicked="#MovetoStudentProfile" prefHeight="70.0" prefWidth="380.0" style="-fx-background-color: #7C7A7A;" text="Student  Profile" textFill="#e5e5e5">
         <font>
            <Font name="System Bold" size="25.0" />
         </font>
         <graphic>
            <ImageView fitHeight="36.0" fitWidth="35.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../image/icons8_Student_Male_96px.png" />
               </image>
            </ImageView>
         </graphic>
      </JFXButton>
