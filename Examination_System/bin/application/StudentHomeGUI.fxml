            <ImageView fitHeight="31.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../image/icons8_Test_Results_100px.png" />
               </image>
            </ImageView>
         </graphic>
      </JFXButton>
      <JFXButton fx:id="JoinClassbtn" buttonType="RAISED" graphicTextGap="20.0" layoutX="480.0" layoutY="110.0" onMouseClicked="#MovetoJoinClass" prefHeight="70.0" prefWidth="380.0" style="-fx-background-color: #7C7A7A;" text="Join Class" textFill="#e5e5e5">
         <font>
            <Font name="System Bold" size="25.0" />
         </font>
         <graphic>
            <ImageView fitHeight="39.0" fitWidth="42.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../image/icons8_Class_100px.png" />
               </image>
            </ImageView>
         </graphic>
      </JFXButton>
      <JFXButton fx:id="ViewClassbtn" buttonType="RAISED" graphicTextGap="20.0" layoutX="480.0" layoutY="240.0" onMouseClicked="#MovetoViewClass" prefHeight="70.0" prefWidth="380.0" style="-fx-background-color: #7C7A7A;" text="View Class" textFill="#e5e5e5">
